{
  "name": "sample-pipeline",
  "description": "A sample Lakechain TypeScript project.",
  "version": "0.4.0",
  "bin": {
    "test": "bin/test.js"
  },
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc -w",
