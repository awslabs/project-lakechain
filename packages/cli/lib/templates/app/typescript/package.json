  },
  "dependencies": {
    "@project-lakechain/core": "^0.4.0",
    "@project-lakechain/s3-event-trigger": "^0.4.0",
    "@project-lakechain/pdf-text-converter": "^0.4.0",
    "@project-lakechain/s3-storage-connector": "^0.4.0",
